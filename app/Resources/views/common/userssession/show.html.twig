{% extends 'base.html.twig' %}

{% block body %}
    <h1>Userssession</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ usersSession.id }}</td>
            </tr>
            <tr>
                <th>Sessiontoken</th>
                <td>{{ usersSession.sessionToken }}</td>
            </tr>
            <tr>
                <th>Ip_client</th>
                <td>{{ usersSession.ipclient }}</td>
            </tr>
            <tr>
                <th>Date</th>
                <td>{% if usersSession.date %}{{ usersSession.date|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('common_userssession_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('common_userssession_edit', { 'id': usersSession.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
